{
	"dependencies": {
		"classnames": "^2.2.6",
		"preact": "^8.2.9",
		"prop-types": "^15.6.2"
	},
	"devDependencies": {
		"eslint": "^4.19.1",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-prettier": "^2.6.0",
		"prettier": "^1.12.1"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es6": true,
			"node": true
		},
		"extends": [
			"plugin:prettier/recommended"
		],
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module"
		},
		"plugins": [
			"prettier"
		],
		"rules": {
			"indent": [
				2,
				"tab"
			],
			"prefer-const": 2,
			"prettier/prettier": 2,
			"quotes": [
				2,
				"single",
				{
					"avoidEscape": true
				}
			]
		}
	},
	"files": [
		"src"
	],
	"main": "src/js/index.js",
	"name": "liferay-help-center-megamenu",
	"prettier": {
		"bracketSpacing": false,
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"private": true,
	"scripts": {
		"format": "prettier --write 'src/js/**/*.js'"
	},
	"version": "1.0.0"
}
